{
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "www.labtech-rh.com.br"
          }
        ],
        "destination": "https://labtech-rh.com.br/$1",
        "permanent": true
      },
      {
        "source": "/sobre",
        "destination": "/#sobre",
        "permanent": true
      },
      {
        "source": "/sobre.html",
        "destination": "/#sobre",
        "permanent": true
      },
      {
        "source": "/sobre/:path*",
        "destination": "/#sobre",
        "permanent": true
      },
  
      {
        "source": "/servicos",
        "destination": "/#servicos",
        "permanent": true
      },
      {
        "source": "/servicos.html",
        "destination": "/#servicos",
        "permanent": true
      },
      {
        "source": "/servicos/:path*",
        "destination": "/#servicos",
        "permanent": true
      },
  
      {
        "source": "/contato",
        "destination": "/#contato",
        "permanent": true
      },
      {
        "source": "/contato.html",
        "destination": "/#contato",
        "permanent": true
      },
      {
        "source": "/contato/:path*",
        "destination": "/#contato",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/sitemap.xml",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
            }
        ]
      }
    ]
  }
  